{
  "name": "@easyin/ui",
  "version": "0.0.0",
  "sideEffects": [
    "**/*.css"
  ],
  "files": [
    "dist"
  ],
  "exports": {
    "./styles.css": "./dist/index.css",
    "./card": "./src/card.tsx"
  },
  "license": "MIT",
  "scripts": {
    "build": "tailwindcss -i ./src/styles.css -o ./dist/index.css",
    "lint": "eslint \"src/**/*.ts*\"",
    "dev": "tailwindcss -i ./src/styles.css -o ./dist/index.css --watch",
    "type-check": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@easyin/eslint-config": "workspace:*",
    "@easyin/tailwind-config": "workspace:*",
    "@easyin/typescript-config": "workspace:*",
    "@types/react": "^18.2.61",
    "autoprefixer": "^10.4.18",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "eslint": "^8.57.0",
    "typescript": "^5.3.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
